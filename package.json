{
  "name": "payrollcalculator",
  "version": "1.0.0",
  "description": "Systeme qui déterimine le Salaire de base sur une longue pèriode de temps pour la Tunisie suivant les convensions de l'Etat tunisien",
  "main": "./dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "go:dev": "webpack --config webpack.main.config.js --env development && webpack serve --config webpack.renderer.config.js --env development",
    "start:dev": "webpack --config webpack.main.config.js --env development && webpack-dev-server --config webpack.renderer.config.js --env development",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "build": "yarn build:main && yarn build:renderer",
    "electron": "electron .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "electron-rebuild": "electron-rebuild",
    "postinstall": "electron-builder install-app-deps && electron-rebuild"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/feridHelali/PayrollCalculator.git"
  },
  "keywords": [
    "Paie",
    "Salaire",
    "Tunisie",
    "Convenstions Collectives Sectorielle"
  ],
  "author": "Ferid HELALI",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/feridHelali/PayrollCalculator/issues"
  },
  "homepage": "https://github.com/feridHelali/PayrollCalculator#readme",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@electron-forge/cli": "^7.4.0",
    "@electron-forge/maker-deb": "^7.4.0",
    "@electron-forge/maker-rpm": "^7.4.0",
    "@electron-forge/maker-squirrel": "^7.4.0",
    "@electron-forge/maker-zip": "^7.4.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.4.0",
    "@electron-forge/plugin-fuses": "^7.4.0",
    "@electron/fuses": "^1.8.0",
    "@testing-library/dom": "^10.2.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/redux-mock-store": "^1.0.6",
    "@types/uuid": "^10.0.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "electron": "^30.0.1",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock": "^29.7.0",
    "node-loader": "^2.0.0",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.77.8",
    "sass-loader": "^16.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@reduxjs/toolkit": "^2.2.5",
    "@types/jest": "^29.5.12",
    "better-sqlite3": "^11.3.0",
    "electron-builder": "^24.13.3",
    "electron-squirrel-startup": "^1.0.1",
    "framer-motion": "^11.2.10",
    "html-loader": "^5.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.2.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.23.1",
    "redux": "^5.0.1",
    "reflect-metadata": "^0.2.2",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.20",
    "uuid": "^10.0.0"
  },
  "config": {
    "forge": {
      "extraResource": [
        {
          "from": "node_modules/sqlite3/lib/binding/",
          "to": "node_modules/sqlite3/lib/binding/",
          "filter": [
            "**/*"
          ]
        }
      ],
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "build": {
    "appId": "tn.alfacomputers.payrollcalculator",
    "files": [
      "**/*"
    ],
    "build": {
      "extraResources": [
        {
          "from": "node_modules/sqlite3/lib/binding/",
          "to": "node_modules/sqlite3/lib/binding/",
          "filter": [
            "**/*"
          ]
        }
      ]
    }
  }
}
